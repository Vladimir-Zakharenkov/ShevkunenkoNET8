@* Listing 5.5 The contents of the Index.cshtml file in the Views/Home folder *@

@model IEnumerable<string>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Language Features</title>
</head>
<body>
    <ul>
        @foreach (string s in Model)
        {
            <li>@s</li>
        }
    </ul>
</body>
</html>

@*-------------------------------------------------------*@